{
  "name": "kebacorp/arrayexcelbuilder",
  "description": "The extension creates an Excel file from the data array.",
  "keywords": [
    "spreadsheet",
    "excel",
    "libreoffice",
    "export",
    "xls",
    "xlsx"
  ],
  "homepage": "https://github.com/KebaCorp/ArrayExcelBuilder",
  "type": "extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Abek Narynov",
      "homepage": "https://www.facebook.com/abeknarynov"
    }
  ],
  "support": {
    "source": "https://github.com/KebaCorp/ArrayExcelBuilder"
  },
  "minimum-stability": "stable",
  "require": {
    "php" : ">=5.6",
    "phpoffice/phpspreadsheet": "~1.1",
    "dompdf/dompdf": "^0.8.3",
    "mpdf/mpdf": "^7.1.9",
    "tecnickcom/tcpdf": "^6.2.13"
  },
  "require-dev": {
    "symfony/cache": "^4.3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "KebaCorp\\ArrayExcelBuilder\\": "src",
      "app\\": ""
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
